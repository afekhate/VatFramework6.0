<!DOCTYPE html>

<html class="loading" lang="en" data-textdirection="ltr">
<!-- BEGIN: Head-->

<head>

    <!-- META DATA -->
    <meta charset="UTF-8">
    <meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Federal Road Safety Corps (Nigeria) :: Inventory">
    <meta name="author" content="Federal Road Safety Corps (Nigeria)">
    <meta name="keywords" content="admin,admin dashboard,">
    <!-- FAVICON -->
    <link rel="shortcut icon" type="image/x-icon" href="#" />

    <title>@ViewData["Title"]</title>
    <!-- BOOTSTRAP CSS -->
    <link id="style" href="~/frscassets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <!-- STYLE CSS -->
    <link href="~/frscassets/css/style.css" rel="stylesheet" />
    <link href="~/frscassets/css/dark-style.css" rel="stylesheet" />
    <link href="~/frscassets/css/transparent-style.css" rel="stylesheet">
    <link href="~/frscassets/css/skin-modes.css" rel="stylesheet" />

    <!--- FONT-ICONS CSS -->
    <link href="~/frscassets/css/icons.css" rel="stylesheet" />

    <!-- COLOR SKIN CSS -->
    <link id="theme" rel="stylesheet" type="text/css" media="all" href="~/frscassets/colors/color1.css" />


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <style>
        .danger{
            color:red;
        }
    </style>
</head>


<body class="app sidebar-mini ltr light-mode">
    <!-- GLOBAL-LOADER -->
    <div id="global-loader">
        <img src="~/frscassets/images/loader.svg" class="loader-img" alt="Loader">
    </div>

    <!-- /GLOBAL-LOADER -->
    <!-- PAGE -->
    <div class="page">
        <div class="page-main">
            <!-- app-Header -->
            <partial name="_SecuredHeader" />
            <!-- /app-Header -->
            <!--SIDEBAR-->
            <partial name="_SecuredMenu" />


            <!-- BEGIN: Content-->
            <div class="main-content app-content mt-0">
                <div class="side-app">
                    <div class="main-container container-fluid">
                        @await Component.InvokeAsync("NToastNotify")

                        @RenderBody()

                    </div>
                </div>

            </div>
            <!-- END: Content-->


        </div>
        <!-- BEGIN: Footer-->
        <partial name="_SecuredFooter" />
        <!-- END: Footer-->
    </div>

    <!-- BACK-TO-TOP -->
    <a href="#top" id="back-to-top"><i class="fa fa-angle-up"></i></a>

    <!-- JQUERY JS -->
    <script src="~/frscassets/js/jquery.min.js"></script>

    <!-- BOOTSTRAP JS -->
    <script src="~/frscassets/plugins/bootstrap/js/popper.min.js"></script>
    <script src="~/frscassets/plugins/bootstrap/js/bootstrap.min.js"></script>

    <!-- SPARKLINE JS-->
    <script src="~/frscassets/js/jquery.sparkline.min.js"></script>

    <!-- Sticky js -->
    <script src="~/frscassets/js/sticky.js"></script>

    <!-- CHART-CIRCLE JS-->
    <script src="~/frscassets/js/circle-progress.min.js"></script>

    <!-- PIETY CHART JS-->
    <script src="~/frscassets/plugins/peitychart/jquery.peity.min.js"></script>
    <script src="~/frscassets/plugins/peitychart/peitychart.init.js"></script>

    <!-- SIDEBAR JS -->
    <script src="~/frscassets/plugins/sidebar/sidebar.js"></script>

    <!-- Perfect SCROLLBAR JS-->
    <script src="~/frscassets/plugins/p-scroll/perfect-scrollbar.js"></script>
    <script src="~/frscassets/plugins/p-scroll/pscroll.js"></script>
    <script src="~/frscassets/plugins/p-scroll/pscroll-1.js"></script>

    <!-- INTERNAL CHARTJS CHART JS-->
    <script src="~/frscassets/plugins/chart/Chart.bundle.js"></script>
    <script src="~/frscassets/plugins/chart/rounded-barchart.js"></script>
    <script src="~/frscassets/plugins/chart/utils.js"></script>

    <!-- INTERNAL SELECT2 JS -->
    <script src="~/frscassets/plugins/select2/select2.full.min.js"></script>

    <!-- INTERNAL Data tables js-->
    <script src="~/frscassets/plugins/datatable/js/jquery.dataTables.min.js"></script>
    <script src="~/frscassets/plugins/datatable/js/dataTables.bootstrap5.js"></script>
    <script src="~/frscassets/plugins/datatable/dataTables.responsive.min.js"></script>


    <script src="~/frscassets/plugins/datatable/js/jquery.dataTables.min.js"></script>
    <script src="~/frscassets/plugins/datatable/js/dataTables.bootstrap5.js"></script>
    <script src="~/frscassets/plugins/datatable/js/dataTables.buttons.min.js"></script>
    <script src="~/frscassets/plugins/datatable/js/buttons.bootstrap5.min.js"></script>
    <script src="~/frscassets/plugins/datatable/js/jszip.min.js"></script>
    <script src="~/frscassets/plugins/datatable/pdfmake/pdfmake.min.js"></script>
    <script src="~/frscassets/plugins/datatable/pdfmake/vfs_fonts.js"></script>
    <script src="~/frscassets/plugins/datatable/js/buttons.html5.min.js"></script>
    <script src="~/frscassets/plugins/datatable/js/buttons.print.min.js"></script>
    <script src="~/frscassets/plugins/datatable/js/buttons.colVis.min.js"></script>
    <script src="~/frscassets/plugins/datatable/dataTables.responsive.min.js"></script>
    <script src="~/frscassets/plugins/datatable/responsive.bootstrap5.min.js"></script>
    <script src="~/frscassets/js/table-data.js"></script>
    <!-- INTERNAL APEXCHART JS -->
    <script src="~/frscassets/js/apexcharts.js"></script>
    <script src="~/frscassets/plugins/apexchart/irregular-data-series.js"></script>

    <!-- C3 CHART JS -->
    <script src="~/frscassets/plugins/charts-c3/d3.v5.min.js"></script>
    <script src="~/frscassets/plugins/charts-c3/c3-chart.js"></script>

    <!-- CHART-DONUT JS -->
    <script src="~/frscassets/js/charts.js"></script>

    <!-- INTERNAL Flot JS -->
    <script src="~/frscassets/plugins/flot/jquery.flot.js"></script>
    <script src="~/frscassets/plugins/flot/jquery.flot.fillbetween.js"></script>
    <script src="~/frscassets/plugins/flot/chart.flot.sampledata.js"></script>
    <script src="~/frscassets/plugins/flot/dashboard.sampledata.js"></script>

    <!-- INTERNAL Vector js -->
    <script src="~/frscassets/plugins/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/frscassets/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>

    <!-- SIDE-MENU JS-->
    <script src="~/frscassets/plugins/sidemenu/sidemenu.js"></script>

    <!-- INTERNAL INDEX JS -->
    <script src="~/frscassets/js/index1.js"></script>

    <!-- Color Theme js -->
    <script src="~/frscassets/js/themeColors.js"></script>

    <!-- CUSTOM JS -->
    <script src="~/frscassets/js/custom.js"></script>




</body>
<!-- END: Body-->

@RenderSection("scripts", required: false)
@RenderSection("JavaScript", required: false)
</html>